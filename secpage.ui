}

/* Style for labels with neon effect */
QLabel {
    color: #00ffcc; /* Neon aqua */
    font-size: 18px; /* Reduced font size */
    font-weight: bold;
    text-align: center;
    font-family: 'Cyber Alert', sans-serif; /* Cyber Alert font */
}

/* Style for buttons */
QPushButton {
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:1, 
                                stop:0 #1f4037, stop:1 #99f2c8); /* Gradient background */
    color: #1c2833; /* Dark text */
    border: none; /* No border */
    border-radius: 20px; /* Rounded edges */
    padding: 10px 20px; /* Adjusted padding for smaller button */
    font-size: 20px; /* Reduced font size */
    font-weight: bold;
    font-family: 'Brittanic', sans-serif; /* Brittanic font for buttons */
}

/* Hover effect for buttons */
QPushButton:hover {
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:1, 
                                stop:0 #34e89e, stop:1 #0f3443); /* Vibrant gradient */
    color: #ecf0f1; /* Switch text to light for contrast */
}

/* Style for line edits */
QLineEdit {
    background-color: #1c2833; /* Dark background */
    color: #ecf0f1; /* Light text */
    border: 2px solid #00d1b2; /* Neon border */
    border-radius: 12px; /* Adjusted for smaller size */
    padding: 6px 10px; /* Adjusted padding for smaller input */
    font-size: 16px; /* Reduced font size */
    font-family: 'Cyber Alert', sans-serif; /* Cyber Alert font */
}

/* Focus effect for line edits */
QLineEdit:focus {
    border-color: #34e89e; /* Change border color when focused */
}
